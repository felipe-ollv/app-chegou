{
  "expo": {
    "name": "ChegouApp!",
    "slug": "chegou-app",
    "owner": "olvfelipe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/chegouapp_android_icon_512.png",
    "scheme": "chegouapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/chegouapp_splash_screen_1242x2436.png",
      "resizeMode": "cover",
      "backgroundColor": "#0c0c1b"
    },
    
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gusen.chegouapp",
      "buildNumber": "1.0.0",
      "icon": "./assets/images/chegouapp_ios_icon_1024.png",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": ["remote-notification"],
        "NSCameraUsageDescription": "Este aplicativo precisa acessar sua câmera para alterar a foto de perfil.",
        "NSPhotoLibraryUsageDescription": "Este aplicativo precisa acessar suas fotos para selecionar uma imagem de perfil.",
        "NSFaceIDUsageDescription": "Usaremos o Face ID para autenticar você com segurança."
      }
    },
    "android": {
      "package": "com.gusen.chegouapp",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptative.png",
        "backgroundColor": "#0c0c1b"
      },
      "edgeToEdgeEnabled": true,
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.ico"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-web-browser",
      [
        "expo-build-properties",
        {
          "android": {
            "googleServicesFile": "./google-services.json"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e258b308-d015-4620-8429-1993b40f9241"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/e258b308-d015-4620-8429-1993b40f9241"
    }
  }
}
