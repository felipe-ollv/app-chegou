{
  "expo": {
    "name": "ChegouApp!",
    "slug": "chegou-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "chegouapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#0c0c1b"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gusen.chegouapp",
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash.png",
        "backgroundColor": "#0c0c1b",
        "package": "com.gusen.chegouapp",
        "versionCode": 1
      },
      "edgeToEdgeEnabled": true,
      "package": "com.gusen.chegouapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e258b308-d015-4620-8429-1993b40f9241"
      }
    }
  }
}
